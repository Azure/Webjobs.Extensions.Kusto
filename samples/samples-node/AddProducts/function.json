{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "direction": "in",
      "type": "httpTrigger",
      "methods": [
        "post"
      ],
      "route": "addproduct"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "product",
      "type": "kusto",
      "database": "%DATABASE%",
      "direction": "out",
      "tableName": "%PRODUCTS_TABLE_NAME%",
      "connection": "KustoConnectionString"
    }
  ],
  "disabled": false
}